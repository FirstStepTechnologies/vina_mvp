ENV=dev
LOG_LEVEL=INFO

DATABASE_URL=sqlite:///./data/vina.db

LLM_PROVIDER=anthropic
LLM_MODEL=claude-sonnet-4-20250514
LLM_REASONING_MODEL=

# Provider-specific API Keys (set based on LLM_PROVIDER)
ANTHROPIC_API_KEY=
OPENAI_API_KEY=changeme
GEMINI_API_KEY=

# LLM Generation Settings
LLM_MAX_TOKENS=2000
LLM_TEMPERATURE=0.3

# Text-to-Speech (ElevenLabs)
ELEVENLABS_API_KEY=
# Voice ID (optional - defaults to Narrator voice: pFZP5JQG7iQjIQuC4Bku)
# Other options:
#   - pFZP5JQG7iQjIQuC4Bku (Narrator - professional, clear)
#   - 21m00Tcm4TlvDq8ikWAM (Rachel - professional female)
#   - Find more at: https://elevenlabs.io/voice-library
ELEVENLABS_VOICE_ID=pFZP5JQG7iQjIQuC4Bku

# Cloudinary (Video Storage)
CLOUDINARY_CLOUD_NAME=
CLOUDINARY_API_KEY=
CLOUDINARY_API_SECRET=

# Opik (Observability) - optional
OPIK_API_KEY=
OPIK_WORKSPACE=vina-hackathon